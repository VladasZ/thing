[
  {
    "context": "Global",
    "bindings": {
      "ctrl-shift-q": "menu::Confirm",
      "ctrl-shift-w": "menu::SecondaryConfirm"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward",
      "alt-f": "file_finder::Toggle",
      "cmd-e": "task::Spawn",
      "cmd-g": [
        "task::Spawn",
        {
          "task_name": "Lazygit"
        }
      ]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-t": "workspace::NewTerminal",
      "ctrl-shift-t": "theme_selector::Toggle",
      "alt-f": "file_finder::Toggle",
      "cmd-g": [
        "task::Spawn",
        {
          "task_name": "Lazygit"
        }
      ],
      "cmd-e": "task::Spawn",
      "cmd-r": [
        "task::Spawn",
        {
          "task_name": "Run"
        }
      ],
      "cmd-b": [
        "task::Spawn",
        {
          "task_name": "Build"
        }
      ],
      "cmd-1": "project_symbols::Toggle",
      "cmd-p": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  }
]
