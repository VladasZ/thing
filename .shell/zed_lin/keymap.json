[
  {
    "context": "Editor",
    "bindings": {
      "alt-[": "pane::GoBack",
      "alt-]": "pane::GoForward",
      "ctrl-t": "workspace::NewTerminal"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-t": "workspace::NewTerminal",
      "alt-e": "task::Spawn",
      "alt-r": "task::Rerun",
      "alt-f": "file_finder::Toggle",
      "alt-1": "project_symbols::Toggle",
      "alt-shift-t": "theme_selector::Toggle",
      "alt-shift-p": "command_palette::Toggle",
      "ctrl-p": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "alt-1": "project_symbols::Toggle"
    }
  }
]
