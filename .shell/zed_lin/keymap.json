[
  {
    "context": "Editor",
    "bindings": {
      "alt-[": "pane::GoBack",
      "alt-]": "pane::GoForward"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-t": "workspace::NewTerminal",
      "ctrl-shift-t": "theme_selector::Toggle",
      "alt-t": "task::Spawn",
      "alt-r": "task::Rerun",
      "alt-1": "project_symbols::Toggle",
      "ctrl-p": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "alt-1": "project_symbols::Toggle"
    }
  }
]
